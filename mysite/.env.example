# Environment variables for production
# Copy this file to .env and update with your values

# Django Secret Key (generate with: python -c 'import secrets; print(secrets.token_urlsafe(50))')
DJANGO_SECRET_KEY=change-me-in-production

# Allowed hosts (your PythonAnywhere domain)
DJANGO_ALLOWED_HOSTS=yourusername.pythonanywhere.com

# CSRF trusted origins
DJANGO_CSRF_TRUSTED_ORIGINS=https://yourusername.pythonanywhere.com

# SSL settings (set to True for production)
DJANGO_SECURE_SSL_REDIRECT=False
DJANGO_SESSION_COOKIE_SECURE=False
DJANGO_CSRF_COOKIE_SECURE=False
